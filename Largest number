from functools import cmp_to_key
class Solution(object):
    def largestNumber(self, nums):
        nums = [str(num) for num in nums]
        def compare(a, b):
            if a + b > b + a:
                return -1
            elif a + b < b + a:
                return 1
            else:
                return 0
        nums.sort(key=cmp_to_key(compare))
        result = ''.join(nums)
        if result[0] == '0':
            return '0'
        return result
