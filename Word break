class Solution(object):
    def wordBreak(self, s, wordDict):
        word_set = set(wordDict)
        divide = [False] * (len(s) + 1)
        divide[0] = True
        for i in range(1, len(s) + 1):
            for j in range(i):
                if divide[j] and s[j:i] in word_set:
                    divide[i] = True
                    break
        return divide[len(s)]
